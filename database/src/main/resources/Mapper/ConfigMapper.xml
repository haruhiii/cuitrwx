<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace绑定一个对应的dao/mapper接口-->
<mapper namespace="cn.cuitrwx.database.dao.ConfigDao">
    <select id="getConfigs" resultType="cn.cuitrwx.database.model.ConfigPO" >
        select * from cuitrwx.config  where is_deleted = false
    </select>
</mapper>